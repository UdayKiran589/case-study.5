//to check if the number is fibo or not
#include <stdio.h>
#include<math.h>
int main()
{
    int n;
    printf("enter a number :");
    scanf("%d",&n);
    int a=5*n*n+4;
    int b=5*n*n-4;
    int n1=sqrt(a);
    int n2=sqrt(b);
    if(n1*n1==a||n2*n2==b)
    {
        printf("%d is a Fibonacci number",n);
    }
    else{
        printf("%d is not a Fibonacci number",n);
    }

}
